<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  id="Definitions_SatelliteService" 
                  targetNamespace="http://aquawatch.ma/bpmn/satellite">

  <!-- ============================================= -->
  <!-- SATELLITE SERVICE - PROCESSUS MÉTIERS        -->
  <!-- ============================================= -->

  <bpmn:process id="Process_IngestionSatellite" name="Processus d'Ingestion des Données Satellite" isExecutable="true">
    
    <bpmn:startEvent id="Start_Satellite" name="Nouvelle scène Sentinel disponible">
      <bpmn:outgoing>Flow_S1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="Task_TelechargerScene" name="Télécharger scène Sentinel (S1 ou S2)">
      <bpmn:incoming>Flow_S1</bpmn:incoming>
      <bpmn:outgoing>Flow_S2</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_ExtraireMetadonnees" name="Extraire métadonnées (productId, tileId, date acquisition, cloud coverage)">
      <bpmn:incoming>Flow_S2</bpmn:incoming>
      <bpmn:outgoing>Flow_S3</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_CloudCoverage" name="Cloud Coverage > 30%?">
      <bpmn:incoming>Flow_S3</bpmn:incoming>
      <bpmn:outgoing>Flow_S4</bpmn:outgoing>
      <bpmn:outgoing>Flow_S5</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:serviceTask id="Task_RejeterScene" name="Rejeter scène (trop nuageuse)">
      <bpmn:incoming>Flow_S4</bpmn:incoming>
      <bpmn:outgoing>Flow_S6</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_SauvegarderScene" name="Créer SatelliteScene dans satellite_db">
      <bpmn:incoming>Flow_S5</bpmn:incoming>
      <bpmn:outgoing>Flow_S7</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_TypeSatellite" name="Type satellite?">
      <bpmn:incoming>Flow_S7</bpmn:incoming>
      <bpmn:outgoing>Flow_S8</bpmn:outgoing>
      <bpmn:outgoing>Flow_S9</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:serviceTask id="Task_CalculNDWI" name="Calculer NDWI/MNDWI (Sentinel-2)">
      <bpmn:incoming>Flow_S8</bpmn:incoming>
      <bpmn:outgoing>Flow_S10</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_CalculWaterMask" name="Générer Water Mask (Sentinel-1 SAR)">
      <bpmn:incoming>Flow_S9</bpmn:incoming>
      <bpmn:outgoing>Flow_S11</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_JoinCalc" name="">
      <bpmn:incoming>Flow_S10</bpmn:incoming>
      <bpmn:incoming>Flow_S11</bpmn:incoming>
      <bpmn:outgoing>Flow_S12</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:serviceTask id="Task_AssocierStations" name="Associer métriques aux stations (par coordonnées GPS)">
      <bpmn:incoming>Flow_S12</bpmn:incoming>
      <bpmn:outgoing>Flow_S13</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:serviceTask id="Task_SauvegarderMetrics" name="Créer SatelliteMetric pour chaque station">
      <bpmn:incoming>Flow_S13</bpmn:incoming>
      <bpmn:outgoing>Flow_S14</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:exclusiveGateway id="Gateway_JoinEnd" name="">
      <bpmn:incoming>Flow_S6</bpmn:incoming>
      <bpmn:incoming>Flow_S14</bpmn:incoming>
      <bpmn:outgoing>Flow_SEnd</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <bpmn:endEvent id="End_Satellite" name="Traitement terminé">
      <bpmn:incoming>Flow_SEnd</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_S1" sourceRef="Start_Satellite" targetRef="Task_TelechargerScene"/>
    <bpmn:sequenceFlow id="Flow_S2" sourceRef="Task_TelechargerScene" targetRef="Task_ExtraireMetadonnees"/>
    <bpmn:sequenceFlow id="Flow_S3" sourceRef="Task_ExtraireMetadonnees" targetRef="Gateway_CloudCoverage"/>
    <bpmn:sequenceFlow id="Flow_S4" name="Oui (> 30%)" sourceRef="Gateway_CloudCoverage" targetRef="Task_RejeterScene"/>
    <bpmn:sequenceFlow id="Flow_S5" name="Non (≤ 30%)" sourceRef="Gateway_CloudCoverage" targetRef="Task_SauvegarderScene"/>
    <bpmn:sequenceFlow id="Flow_S6" sourceRef="Task_RejeterScene" targetRef="Gateway_JoinEnd"/>
    <bpmn:sequenceFlow id="Flow_S7" sourceRef="Task_SauvegarderScene" targetRef="Gateway_TypeSatellite"/>
    <bpmn:sequenceFlow id="Flow_S8" name="Sentinel-2" sourceRef="Gateway_TypeSatellite" targetRef="Task_CalculNDWI"/>
    <bpmn:sequenceFlow id="Flow_S9" name="Sentinel-1" sourceRef="Gateway_TypeSatellite" targetRef="Task_CalculWaterMask"/>
    <bpmn:sequenceFlow id="Flow_S10" sourceRef="Task_CalculNDWI" targetRef="Gateway_JoinCalc"/>
    <bpmn:sequenceFlow id="Flow_S11" sourceRef="Task_CalculWaterMask" targetRef="Gateway_JoinCalc"/>
    <bpmn:sequenceFlow id="Flow_S12" sourceRef="Gateway_JoinCalc" targetRef="Task_AssocierStations"/>
    <bpmn:sequenceFlow id="Flow_S13" sourceRef="Task_AssocierStations" targetRef="Task_SauvegarderMetrics"/>
    <bpmn:sequenceFlow id="Flow_S14" sourceRef="Task_SauvegarderMetrics" targetRef="Gateway_JoinEnd"/>
    <bpmn:sequenceFlow id="Flow_SEnd" sourceRef="Gateway_JoinEnd" targetRef="End_Satellite"/>

  </bpmn:process>

  <!-- DIAGRAMME BPMN -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_Satellite">
    <bpmndi:BPMNPlane id="BPMNPlane_Satellite" bpmnElement="Process_IngestionSatellite">
      
      <bpmndi:BPMNShape id="Start_Satellite_di" bpmnElement="Start_Satellite">
        <dc:Bounds x="152" y="232" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_TelechargerScene_di" bpmnElement="Task_TelechargerScene">
        <dc:Bounds x="240" y="210" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_ExtraireMetadonnees_di" bpmnElement="Task_ExtraireMetadonnees">
        <dc:Bounds x="390" y="210" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_CloudCoverage_di" bpmnElement="Gateway_CloudCoverage" isMarkerVisible="true">
        <dc:Bounds x="545" y="225" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_RejeterScene_di" bpmnElement="Task_RejeterScene">
        <dc:Bounds x="640" y="100" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_SauvegarderScene_di" bpmnElement="Task_SauvegarderScene">
        <dc:Bounds x="640" y="310" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_TypeSatellite_di" bpmnElement="Gateway_TypeSatellite" isMarkerVisible="true">
        <dc:Bounds x="795" y="325" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_CalculNDWI_di" bpmnElement="Task_CalculNDWI">
        <dc:Bounds x="890" y="250" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_CalculWaterMask_di" bpmnElement="Task_CalculWaterMask">
        <dc:Bounds x="890" y="380" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_JoinCalc_di" bpmnElement="Gateway_JoinCalc" isMarkerVisible="true">
        <dc:Bounds x="1045" y="325" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AssocierStations_di" bpmnElement="Task_AssocierStations">
        <dc:Bounds x="1140" y="310" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_SauvegarderMetrics_di" bpmnElement="Task_SauvegarderMetrics">
        <dc:Bounds x="1290" y="310" width="100" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Gateway_JoinEnd_di" bpmnElement="Gateway_JoinEnd" isMarkerVisible="true">
        <dc:Bounds x="1445" y="225" width="50" height="50"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="End_Satellite_di" bpmnElement="End_Satellite">
        <dc:Bounds x="1552" y="232" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <!-- Edges -->
      <bpmndi:BPMNEdge id="Flow_S1_di" bpmnElement="Flow_S1">
        <di:waypoint x="188" y="250"/>
        <di:waypoint x="240" y="250"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S2_di" bpmnElement="Flow_S2">
        <di:waypoint x="340" y="250"/>
        <di:waypoint x="390" y="250"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S3_di" bpmnElement="Flow_S3">
        <di:waypoint x="490" y="250"/>
        <di:waypoint x="545" y="250"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S4_di" bpmnElement="Flow_S4">
        <di:waypoint x="570" y="225"/>
        <di:waypoint x="570" y="140"/>
        <di:waypoint x="640" y="140"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S5_di" bpmnElement="Flow_S5">
        <di:waypoint x="570" y="275"/>
        <di:waypoint x="570" y="350"/>
        <di:waypoint x="640" y="350"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S6_di" bpmnElement="Flow_S6">
        <di:waypoint x="740" y="140"/>
        <di:waypoint x="1470" y="140"/>
        <di:waypoint x="1470" y="225"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S7_di" bpmnElement="Flow_S7">
        <di:waypoint x="740" y="350"/>
        <di:waypoint x="795" y="350"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S8_di" bpmnElement="Flow_S8">
        <di:waypoint x="820" y="325"/>
        <di:waypoint x="820" y="290"/>
        <di:waypoint x="890" y="290"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S9_di" bpmnElement="Flow_S9">
        <di:waypoint x="820" y="375"/>
        <di:waypoint x="820" y="420"/>
        <di:waypoint x="890" y="420"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S10_di" bpmnElement="Flow_S10">
        <di:waypoint x="990" y="290"/>
        <di:waypoint x="1070" y="290"/>
        <di:waypoint x="1070" y="325"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S11_di" bpmnElement="Flow_S11">
        <di:waypoint x="990" y="420"/>
        <di:waypoint x="1070" y="420"/>
        <di:waypoint x="1070" y="375"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S12_di" bpmnElement="Flow_S12">
        <di:waypoint x="1095" y="350"/>
        <di:waypoint x="1140" y="350"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S13_di" bpmnElement="Flow_S13">
        <di:waypoint x="1240" y="350"/>
        <di:waypoint x="1290" y="350"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_S14_di" bpmnElement="Flow_S14">
        <di:waypoint x="1390" y="350"/>
        <di:waypoint x="1470" y="350"/>
        <di:waypoint x="1470" y="275"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_SEnd_di" bpmnElement="Flow_SEnd">
        <di:waypoint x="1495" y="250"/>
        <di:waypoint x="1552" y="250"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>
